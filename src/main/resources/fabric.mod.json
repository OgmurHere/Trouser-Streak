{
  "schemaVersion": 1,
  "id": "streak-addon",
  "version": "${version}",
  "name": "TrouserStreak",
  "description": "Trouser-Streak is a compilation of modules, updated to the latest version and optimized for maximum grief. I did not make all of these.",
  "authors": [
    "etianl, and various authors"
  ],
  "contact": {
    "repo": "https://github.com/Etianl/Trouser-Streak/"
  },
  "icon": "assets/icon/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "pwn.noobs.trouserstreak.Trouser"
    ]
  },
  "mixins": [
    "streak.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,0,255",
    "github:sha": "${gh_hash}"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
